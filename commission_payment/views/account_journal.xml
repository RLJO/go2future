<odoo>
    <record id="account_commission" model="ir.ui.view">
        <field name="name">account.commission</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='l10n_ar_share_sequences']" position="after">
                    <field name="commission" string="commission"/>
            </xpath>
        </field>
    </record>
</odoo>
