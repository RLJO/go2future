<odoo>
    <record id="product_template_commission" model="ir.ui.view">
        <field name="name">product.template.commission</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_income_id']" position="after">
                    <field name="commission" string="commission"/>
            </xpath>
        </field>
    </record>
</odoo>
