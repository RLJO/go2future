<odoo>
  <data>
      <record model="ir.ui.view" id="g2f_view_move_form">
          <field name="name">g2f.account.move.form</field>
          <field name="model">account.move</field>
          <field name="inherit_id" ref="account.view_move_form"/>
          <field name="arch" type="xml">
              <xpath expr="//notebook" position="inside">
                  <page name="einvoice" string="Electronic Invoice">
                      <group>
                          <field name="einvoice"/>
                          <field name="date_einvoice"/>
                          <field name="cae_number"/>
                          <field name="ei_qr_code"/>
                          <field name="ei_barcode"/>
                          <field name="ei_xml_file"/>
                          <field name="ei_pdf"/>
                      </group>
                  </page>
              </xpath>
          </field>
      </record>
  </data>
</odoo>